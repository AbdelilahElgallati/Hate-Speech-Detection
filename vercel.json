{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python@2.0.0",
            "config": {
                "maxLambdaSize": "50mb",
                "installCommand": "pip install -r requirements.txt"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}