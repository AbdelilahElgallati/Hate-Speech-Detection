{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python@2.0.0",
            "config": {
                "maxLambdaSize": "50mb",
                "runtime": "python3.9",
                "installCommand": "python3.9 -m pip install --upgrade pip && pip install -r requirements.txt"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.9.18"
    }
}
